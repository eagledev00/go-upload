    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "env": {
                "UPLOAD_KEY": "testing-only",
                "STORAGE_PATH": "storage",
                "FILENAME_LENGTH": "6",
                "MAX_UPLOAD_SIZE_IN_MB": "100",
                "LISTEN_ADDRESS": "0.0.0.0:9000",
                "ENABLE_WEBFORM": "true",
                "PUBLIC_ROOT": "http://127.0.0.1:9000/"
            }
        }
    ]
}